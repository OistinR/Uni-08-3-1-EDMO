  <link name="arm1">
  		<visual>
		<!-- important values: -->
		    <origin xyz="-0.2345 0 -.143" rpy="0 0 0"/>
			<geometry>
				<mesh filename="Module_Arm(1).stl" scale="0.1"/>
			</geometry>
			<material name="Cyan1">
	       		<color rgba="0.6 0.15 0.9 1.0"/>
