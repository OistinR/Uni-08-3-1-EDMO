<robot name="robot">

	<!-- * * * Link Definitions * * * -->

 	<link name="base_link">
		<visual>
		    <origin xyz="0 0 0" rpy="0 0 0"/>
         
			<geometry>
				<box size=".67 .67 .3"/>
			</geometry>
			<material name="c">
	       		<color rgba="0 0.9 0.9 1.0"/>
	     	</material>
		</visual>	

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size=".67 .67 .3"/>
      </geometry>
      </collision>

	</link>
